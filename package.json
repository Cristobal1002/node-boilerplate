{
  "name": "mordcai-api",
  "version": "1.0.0",
  "description": "Backend for the Mordcai software",
  "main": "app.js",
  "scripts": {
    "start": "node app.js",
    "dev": "nodemon app.js",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest --testPathPattern=unit",
    "test:integration": "jest --testPathPattern=integration",
    "lint": "eslint src/**/*.js",
    "lint:fix": "eslint src/**/*.js --fix",
    "format": "prettier --write \"src/**/*.js\"",
    "format:check": "prettier --check \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Cristobal1002/node-boilerplate.git"
  },
  "keywords": [
    "node",
    "express",
    "api",
    "boilerplate"
  ],
  "author": "",
  "license": "ISC",
  "type": "module",
  "bugs": {
    "url": "https://github.com/Cristobal1002/node-boilerplate/issues"
  },
  "homepage": "https://github.com/Cristobal1002/node-boilerplate#readme",
  "dependencies": {
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.4.1",
    "express-validator": "^7.3.0",
    "helmet": "^8.0.0",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "pino": "^9.6.0",
    "pino-pretty": "^13.0.0",
    "sequelize": "^6.37.7"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "eslint": "^9.17.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
